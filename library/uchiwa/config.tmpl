{
  "sensu": [
    {
      "name": "{{ .Env.SENSU_NAME }}",
      "host": "{{ .Env.SENSU_API_HOST }}",
      "port": {{ default .Env.SENSU_API_PORT "4567" }}
    }
  ],
  "uchiwa": {
    "host": "{{ default .Env.UCHIWA_HOST "0.0.0.0" }}",
    "port": {{ default .Env.UCHIWA_PORT "80" }},
    "refresh": {{ default .Env.UCHIWA_REFRESH "5" }},
    "user": "{{ .Env.HTTP_USER }}",
    "pass": "{{ .Env.HTTP_PASS }}"
  }
}
